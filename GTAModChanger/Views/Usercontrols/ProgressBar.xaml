<UserControl x:Class="GTAModChanger.Views.Usercontrols.ProgressBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GTAModChanger.Views.Usercontrols"
             mc:Ignorable="d" 
             DataContext="{Binding Progress,Source={StaticResource Locator}}"
             d:DesignHeight="450" d:DesignWidth="800" Background="White">
    <Grid>
        <ProgressBar Value="{Binding progressValue}" Minimum="0" Maximum="100"></ProgressBar> 
    </Grid>
</UserControl>
